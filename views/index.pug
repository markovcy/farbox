include header.pug
body
    p TEST
    div.wrapper
        header.header
            div(class="header_container _container")
                a(href="", class="header_logo") FARBOX
                nav(class="header_menu menu")
        div.catalog__page
            div(class="catalog__container _container")
                div.catalog__container
                    each val2, index2 in category
                        h2
                          a(
                            href="/cat?id="+val2['id']
                          ) #{val2['category']}
                        div.catalog__body
                          each val, index in goods
                            if val2['id'] === val['category']
                              include card.pug
        include nav
    include footer